{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "app.js" },
    { "src": "/api-docs/(.*)", "dest": "app.js" },
    { "src": "/(.*)", "dest": "app.js" }
  ],
  "env": {
    "PORT": "3001",
    "NODE_ENV": "production"
  }
}
